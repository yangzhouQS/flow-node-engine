# Application
NODE_ENV=development
PORT=3000
APP_NAME=flow-node-engine
APP_VERSION=1.0.0

# Database
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=root
DB_PASSWORD=root
DB_DATABASE=flow_engine
DB_SYNCHRONIZE=true
DB_LOGGING=true

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=2h

# Cache
CACHE_TTL=3600

# Logging
LOG_DIR=logs
LOG_LEVEL=info

# Bull Queue
BULL_REDIS_HOST=localhost
BULL_REDIS_PORT=6379
BULL_REDIS_PASSWORD=
